// 사용자 정의 함수 

public class Main {
	
	// 반환형, 함수명, 매개변수 
	public static int function(int a, int b, int c) {
		int min;
		if(a>b) {
			if(b<c) {
				min = c;
			} else {
				min = b;
			}
		} else {
			if(a<c) {
				min = c;
			} else {
				min = a;
			}
		}
		
		return 0;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

}
