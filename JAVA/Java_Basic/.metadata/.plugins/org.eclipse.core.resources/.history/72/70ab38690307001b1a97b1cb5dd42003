// 인터페이스(interface)

public class Main implements Dog {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void crying() {
		// TODO Auto-generated method stub
		
		System.out.println("bow wow!");
	}

	@Override
	public void show() {
		// TODO Auto-generated method stub
		
		System.out.println("Hello");
	}

}
