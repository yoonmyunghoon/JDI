// 사용자 정의 함수

public class Main {
	
	public static int function(int number, int k) {
		for(int i = 1; i <= number; i++) {
			if(number%i==0) {
				k--;
				if(k==0) {
					return i;
				}
			}
		}
		return -1;
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

}
